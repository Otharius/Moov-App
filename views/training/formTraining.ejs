        <% if (userData.templates.templates[l].fields[m] == 'exercice') { %>
           <select name="" id="">


           </select>
        <% } %>

        <% if (userData.templates.templates[l].fields[m] == 'repetition') { %>
            <input type="number" name="repetition" placeholder="repetition">
        <% } %>

        <% if (userData.templates.templates[l].fields[m] == 'serie') { %>
            <input type="number" name="serie" placeholder="serie">
        <% } %>

        <% if (userData.templates.templates[l].fields[m] == 'pause') { %>
