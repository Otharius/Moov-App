                                    <input type="radio" class="radio" id=" <%=exMuscu[i]%> " name="category" value=" <%=exMuscu[i]%>">
                                    <label for=" <%=exMuscu[i]%>"> <%=exMuscu[i]%></label>
                                </div>
                            <% } %>
                                                            
                        </div>
                
                        <div class="selected">
                            Sélectionner un exercice
                        </div>
                
                        <div class="search-box">
                            <input type="text" placeholder="Trouver un exercice" name="exercice" id="exerciceChoice" value="" autocomplete="off"/>
                        </div>

                    </div>

                    <div class="content_precision">
                        <div class="exPart1">
                            Nombre de répétition:
                            <input type="number" name="repetition" id="" min="1" max="50" value="1" required><br>

                            Nombre de série:
                            <input type="number" name="serie" id="" min="1" max="10" value="1" required><br>
                        </div>

                        <div class="exPart2">
                            Temps de repos:
                            <input type="number" name="reposMin" id="" min="0" max="30" value="0" required> Minute

                            <input type="number" name="reposSec" id="" min="0" max="59" value="30" required> Seconde
                            <br><br>
                        </div>  
                    </div>
